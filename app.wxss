/**app.wxss**/
.flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.flexs {
  flex: 1;
}
.ellipse {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.mask{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .4);
}
.nolist{
  font-size: 22rpx;
  color: #949494;
  text-align: center;
}
.nolist image{
  width: 246rpx;
  height: 178rpx;
  margin: 0 auto 20rpx;
}